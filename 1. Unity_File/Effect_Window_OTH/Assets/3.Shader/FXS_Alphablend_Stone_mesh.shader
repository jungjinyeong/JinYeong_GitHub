			#else
				float staticSwitch23 = _Opaciy;
			#endif
			o.Alpha = ( i.vertexColor.a * saturate( staticSwitch23 ) );
		}

		ENDCG
		CGPROGRAM
		#pragma surface surf Standard keepalpha fullforwardshadows nometa noforwardadd 

		ENDCG
